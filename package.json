{
  "name": "joggps-ai-coaching-backend",
  "version": "1.0.0",
  "private": true,
  "description": "JogGPS AI Coaching Backend - Self-hosted on Coolify",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "node scripts/local-test.js",
    "test:api:remote": "BASE_URL=$npm_config_url node scripts/local-test.js",
    "docker:build": "docker build -t joggps-ai-coaching-backend:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local joggps-ai-coaching-backend:latest"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redis": "^4.6.0",
    "axios": "^1.6.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}